<?xml version="1.0"?>
<faces-config version="1.2" xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xi="http://www.w3.org/2001/XInclude"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">
 <managed-bean>
  <managed-bean-name>LoginBean</managed-bean-name>
  <managed-bean-class>trabalho.visao.managedbean.LoginBean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
  <managed-property>
   <property-name>welcomeImage</property-name>
   <value>/imagens/bem_vindo.jpg</value>
  </managed-property>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>cadastroFuncionarioBean</managed-bean-name>
  <managed-bean-class>trabalho.visao.managedbean.CadastroFuncionarioBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>exibirMedicosBean</managed-bean-name>
  <managed-bean-class>trabalho.visao.managedbean.ExibirMedicosBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>cadastroPacienteBean</managed-bean-name>
  <managed-bean-class>trabalho.visao.managedbean.CadastroPacienteBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>agendamentoConsultaBean</managed-bean-name>
  <managed-bean-class>trabalho.visao.managedbean.AgendamentoConsultaBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>atendimentoPacienteBean</managed-bean-name>
  <managed-bean-class>trabalho.visao.managedbean.AtendimentoPacienteBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>exibirAtendentesBean</managed-bean-name>
  <managed-bean-class>trabalho.visao.managedbean.ExibirAtendentesBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <navigation-rule>
  <from-view-id>/pages/login/frm_login.jsp</from-view-id>
  <navigation-case>
   <from-outcome>principal</from-outcome>
   <to-view-id>/pages/principal/frm_principal.jsp</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <navigation-case>
   <from-outcome>exibirAtendentes</from-outcome>
   <to-view-id>/pages/cadastro/frm_lista_atendentes.jsp</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>exibirMedicos</from-outcome>
   <to-view-id>/pages/cadastro/frm_edita_medico.jsp</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>agendarConsulta</from-outcome>
   <to-view-id>/pages/consulta/frm_agendamento_consulta.jsp</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>cadastroPaciente</from-outcome>
   <to-view-id>/pages/cadastro/frm_cadastro_paciente.jsp</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>atenderPaciente</from-outcome>
   <to-view-id>/pages/consulta/frm_atendimento_consulta.jsp</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>cadastroAtendente</from-outcome>
   <to-view-id>/pages/cadastro/frm_cadastro_atendente.jsp</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>cadastroMedico</from-outcome>
   <to-view-id>/pages/cadastro/frm_cadastro_medico.jsp</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>paginaPrincipal</from-outcome>
   <to-view-id>/pages/principal/frm_principal.jsp</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>login</from-outcome>
   <to-view-id>/pages/login/frm_login.jsp</to-view-id>
  </navigation-case>
 </navigation-rule>
 <application>
  <resource-bundle>
   <base-name>trabalho.visao.resources.i18n</base-name>
   <var>msgs</var>
  </resource-bundle>
 </application>
</faces-config>

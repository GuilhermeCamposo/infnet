<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="trabalho.modelo.entidades" >
 <class name="Paciente">
  <id name="id">
   <generator class="identity"/>
  </id>
  <!-- Propriedades da classe -->
  <property generated="never" lazy="false" name="nome"/>
  <property generated="never" lazy="false" name="cpf" unique="true" />
  <property generated="never" lazy="false" name="telefone"/>

 </class>
</hibernate-mapping>

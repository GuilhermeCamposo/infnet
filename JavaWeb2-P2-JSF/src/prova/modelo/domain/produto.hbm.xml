<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="prova.modelo.entidade">
	<class name="Produto">
		<id name="id" unsaved-value="null">
			<generator class="identity" />
		</id>

		<!-- Propriedades da classe -->
		<property generated="never" lazy="false" name="nome" length="50" />
		<property generated="never" lazy="false" name="modelo" length="50" />
		<property generated="never" lazy="false" name="preco"  />
	</class>
</hibernate-mapping>
